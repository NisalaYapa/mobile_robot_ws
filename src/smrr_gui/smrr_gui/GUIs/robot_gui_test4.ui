  <property name="styleSheet">
   <string notr="true">/* Main application background - lighter version */
QWidget {
    background-color: #E0F7FA;  /* very light bluish background */
}

/* Buttons */
QPushButton {
    background-color: #00C9A7;  /* bluish-green */
    color: #FFFFFF;  /* white text */
    border: 2px solid #008B74;  /* darker border */
    border-radius: 8px;
    padding: 8px 16px;
    font-size: 14px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #00E6D3;  /* lighter bluish-green */
    border-color: #00C9A7;
}

QPushButton:pressed {
    background-color: #008B74;  /* darker bluish-green */
}

/* Labels */
QLabel {
    color: #00796B;  /* darker bluish-green for better contrast */
    font-size: 14px;
}

QLabel:hover {
    color: #00C9A7;  /* standard bluish-green */
}

/* Line edits */
QLineEdit {
    background-color: #FFFFFF;  /* white background */
    color: #00796B;  /* darker bluish-green */
    border: 1px solid #00C9A7;
    border-radius: 4px;
    padding: 5px;
    selection-background-color: #00C9A7;
    selection-color: #FFFFFF;
}

/* Checkboxes and radio buttons */
QCheckBox, QRadioButton {
    color: #00796B;
    spacing: 5px;
}

QCheckBox::indicator, QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #00C9A7;
}

QCheckBox::indicator:checked, QRadioButton::indicator:checked {
    background-color: #00C9A7;
    border: 1px solid #008B74;
}

/* Scrollbars */
QScrollBar:vertical {
    background: #B2EBF2;  /* very light blue */
    width: 12px;
}

QScrollBar::handle:vertical {
    background: #00C9A7;
    min-height: 20px;
    border-radius: 6px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background: none;
}

/* Progress bar */
QProgressBar {
    border: 1px solid #00C9A7;
    border-radius: 5px;
    text-align: center;
    color: #00796B;
    background: #FFFFFF;
}

QProgressBar::chunk {
    background-color: #00C9A7;
}

/* Combo box */
QComboBox {
    background-color: #FFFFFF;
    color: #00796B;
    border: 1px solid #00C9A7;
    border-radius: 4px;
    padding: 5px;
}

QComboBox:hover {
    border: 1px solid #00E6D3;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left-width: 1px;
    border-left-color: #00C9A7;
    border-left-style: solid;
}

/* Sliders */
QSlider::groove:horizontal {
    height: 8px;
    background: #B2EBF2;
    border-radius: 4px;
}

QSlider::handle:horizontal {
    background: #00C9A7;
    border: 1px solid #008B74;
    width: 16px;
    margin: -4px 0;
    border-radius: 8px;
}

/* Tab widget */
QTabWidget::pane {
    border: 1px solid #00C9A7;
    background: #FFFFFF;
}

QTabBar::tab {
    background: #B2EBF2;
    color: #00796B;
    padding: 8px;
    border: 1px solid #00C9A7;
    border-bottom: none;
}

QTabBar::tab:selected {
    background: #FFFFFF;
    color: #00C9A7;
    border-bottom: 1px solid #FFFFFF;
}

/* Group boxes */
QGroupBox {
    border: 1px solid #00C9A7;
    border-radius: 5px;
    margin-top: 1ex;
    color: #00796B;
    font-weight: bold;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 3px;
}</string>
